<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="dmy.tipboard.common.dao.ChabunDAO">

<select id="getBoardChabun" resultType="DmyTipboardVO">
	SELECT NVL(MAX(SUBSTR(A.DTB_NO, -4)), 0) + 1  DTB_NO FROM DMY_TIPBOARD A
</select>


</mapper>			